{
  "name": "browserify-banner",
  "version": "1.0.11",
  "description": "Add a comment (and/or code) to the top of your Browserify bundle",
  "keywords": [
    "browserify",
    "browserify-plugin",
    "banner",
    "header",
    "prelude",
    "preamble",
    "comment",
    "license",
    "version"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/JS-DevTools/browserify-banner",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/browserify-banner.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib test/fixtures test/specs --fix",
    "test": "mocha && npm run lint",
    "coverage": "nyc --reporter=text --reporter=lcov mocha",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --tag --push --all",
    "release": "npm run upgrade && npm test && npm run bump && npm publish"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "codacy-coverage": "^3.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-config-modular": "^4.2.1",
    "exorcist": "^1.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "npm-check": "^5.9.0",
    "nyc": "^13.0.1",
    "touch": "^3.1.0",
    "uglifyify": "^5.0.1",
    "version-bump-prompt": "^4.2.0"
  },
  "dependencies": {
    "convert-source-map": "^1.6.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "offset-sourcemap-lines": "^1.0.1",
    "ono": "^4.0.9",
    "through2": "^2.0.3"
  },
  "engines": {
    "node": ">=4"
  }
}